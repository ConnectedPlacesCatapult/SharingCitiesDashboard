flask_server:
    # -------------------------------
    # | Flask server configurations |
    # -------------------------------
    host: 0.0.0.0
    port: 5000
    use_debugger: null
    use_reloader: null
    threaded: false
    processes: 1
    passthrough_errors: false
    ssl_crt: null
    ssl_key: null

gunicorn_server:
    # ----------------------------------
    # | Gunicorn server configurations |
    # ----------------------------------
    gunicorn_host: 0.0.0.0
    gunicorn_port: 5000
    gunicorn_workers: 4


jwt_auth:
    # ---------------------------------
    # | JWT Auth Token configurations |
    # ---------------------------------
    JWT_SECRET_KEY: jwt-secret-string
    JWT_BLACKLIST_ENABLED: true
    JWT_BLACKLIST_TOKEN_CHECKS:
        - access
        - refresh

postgres:
    # ------------------------------------
    # | Postgres database configurations |
    # ------------------------------------
    SECRET_KEY: test-secret-key
    DEBUG: true
    db_username: sharingcities
    db_password: sharingcities
    db_name: test_analytics
    db_host: localhost
    db_sql_base_uri: mysql+pymysql
    db_psql_base_uri: postgresql+psycopg2
    SQLALCHEMY_DATABASE_URI:
                "postgresql+psycopg2://sharingcities:sharingcitie\
                s@localhost/test_analytics"
    SQLALCHEMY_TRACK_MODIFICATIONS: false

celery:
    # ---------------------------------
    # | Celery / Redis configurations |
    # ---------------------------------
    BROKER_URL: 'redis://localhost:6379/0'
    CELERY_RESULT_BACKEND: 'redis://localhost:6379/0'

sendgrid:
    # ---------------------------
    # | SendGrid configurations |
    # ---------------------------
    system_password_length: 15
    sender_email: sharedcitiestesting@gmail.com
    email_subject: 'Sharing Cities - Forgot Password'
    html_template: forgot_password_email.html
    text_template:
            "Hi {username}\nYou requested for your password to be reset. Your \
            new system generated password is : {password} You can now login to \
            the Sharing Cities Dashboard with this password.\nIt is \
            recommended you change your password once logged in.\n"
    api_key: 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

